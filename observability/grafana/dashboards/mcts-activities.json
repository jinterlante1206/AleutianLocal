      "pluginVersion": "10.0.0",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "sum(rate(mcts_activity_executions_total[5m])) by (activity)",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "Executions"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "1 - sum(rate(mcts_activity_errors_total[5m])) by (activity) / sum(rate(mcts_activity_executions_total[5m])) by (activity)",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "SuccessRate"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.95, sum(rate(mcts_activity_duration_seconds_bucket[5m])) by (activity, le)) * 1000",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "P95Latency"
        }
      ],
      "title": "Activity Summary",
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "renameByName": {
              "activity": "Activity",
              "Value #Executions": "Executions/sec",
              "Value #P95Latency": "P95 Latency",
              "Value #SuccessRate": "Success Rate"
            }
          }
        }
      ],
      "type": "table"
